                    <li id="subMenu-t_max_acu" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('t_max_acu','@Localizer["GlossaryTMaxAcuName"]','t_max_acu')">@Localizer["GlossaryTMaxAcuName"]</li>
