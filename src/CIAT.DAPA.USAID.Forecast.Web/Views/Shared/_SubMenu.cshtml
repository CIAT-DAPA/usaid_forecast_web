@using CIAT.DAPA.USAID.Forecast.Web.Models.Tools
<div id="secundaryMenu" ng-controller="SubMenuCtrl">
    <div id="leftMenu">

        <div class="leftMenu-wrapper col-md-12">
            <ul class="row">
                <!--<li id="subMenu-{{sb.value}}" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView(sb.value,sb.name,sb.section)">{{sb.name}}</li>-->
                @if ((SectionSite)ViewBag.Section == SectionSite.Climate)
                {
                    <li id="subMenu-forecast" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('forecast','@Localizer["ClimateForecastName"]','forecast')">@Localizer["ClimateForecastName"]</li>
                    <li id="subMenu-precipitation" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('precipitation','@Localizer["ClimateHistoricalPrecipitationName"]','historical')">@Localizer["ClimateHistoricalPrecipitationName"]</li>
                    <li id="subMenu-temperature" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('temperature','@Localizer["ClimateHistoricalTemperatureName"]','historical')">@Localizer["ClimateHistoricalTemperatureName"]</li>
                    <li id="subMenu-solar_radiation" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('solar_radiation','@Localizer["ClimateHistoricalSolarRadiationName"]','historical')">@Localizer["ClimateHistoricalSolarRadiationName"]</li>
                }
                else if ((SectionSite)ViewBag.Section == SectionSite.Crop)
                {
                    <li id="subMenu-yield" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('yield','@Localizer["CropForecasAgroclimatetName"]','forecast')">@Localizer["CropForecasAgroclimatetName"]</li>
