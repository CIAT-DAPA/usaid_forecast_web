                    <li id="subMenu-yield_forecast" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('yield_forecast','@Localizer["ExpertYieldForecastName"]','yield_forecast')">@Localizer["ExpertYieldForecastName"]</li>
                }
                else if ((SectionSite)ViewBag.Section == SectionSite.Glossary)
                {
                    <li id="subMenu-bio_acu" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('bio_acu','@Localizer["GlossaryBioAcuName"]','bio_acu')">@Localizer["GlossaryBioAcuName"]</li>
