                }
                else if ((SectionSite)ViewBag.Section == SectionSite.Expert)
                {
                    <li id="subMenu-geographic" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('geographic','@Localizer["ExpertGeographicName"]','geographic')">@Localizer["ExpertGeographicName"]</li>
                    <li id="subMenu-agronomic" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('agronomic','@Localizer["ExpertAgronomicName"]','agronomic')">@Localizer["ExpertAgronomicName"]</li>
                    <li id="subMenu-climatology" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('climatology','@Localizer["ExpertClimatologyName"]','climatology')">@Localizer["ExpertClimatologyName"]</li>
