                else if ((SectionSite)ViewBag.Section == SectionSite.About)
                {
                    <li id="subMenu-project" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('project','@Localizer["AboutProjectName"]','project')">@Localizer["AboutProjectName"]</li>
