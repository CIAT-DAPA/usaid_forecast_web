                    <li id="subMenu-pro_his" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('pro_his','@Localizer["GlossaryProHisName"]','pro_his')">@Localizer["GlossaryProHisName"]</li>
                    <li id="subMenu-forecast" class="col-md-12 subMenuItem" ng-class="{active: $index == 0}" ng-click="renderView('forecast','@Localizer["GlossaryForecastName"]','forecast')">@Localizer["GlossaryForecastName"]</li>
